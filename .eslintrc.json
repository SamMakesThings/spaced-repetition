{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "linebreak-style": 0,
    "react/jsx-one-expression-per-line": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "react/prop-types": ["error", { "ignore": ["tintColor"] }],
    "global-require": 0,
    "quote-props": 0
  }
}
